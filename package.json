  "license": "MIT",
  "keywords": [
    "typescript",
    "tsconfig"
  ],
  "homepage": "https://github.com/nyarthan/tsconfig?tab=readme-ov-file",
  "repository": "github:nyarthan/tsconfig",
  "bugs": "https://github.com/nyarthan/tsconfig/issues",
  "packageManager": "pnpm@10.15.1",
